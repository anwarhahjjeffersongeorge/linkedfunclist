{
  "name": "linkedfunclist",
  "version": "1.0.0",
  "description": "A linked list whose elements store functions.",
  "main": "./src/index.js",
  "module": "./src/main.js",
  "scripts": {
    "clean": "rm -rf .nyc_output/* coverage/* docs/*",
    "predoc": "rm -rf docs/*",
    "doc": "esdoc",
    "pretest": "clear && standard --fix \"index.js\" \"main.js\"",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=html ava",
    "pretestw": "clear",
    "testw": "ava --watch",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "github:anwarhahjjeffersongeorge/linkedfunclist"
  },
  "keywords": [
    "linked",
    "list",
    "promise",
    "function"
  ],
  "author": "Anwar Hahj Jefferson-George <anwarhahjjeffersongeorge@gmail.com>",
  "license": "CC-BY-SA-4.0",
  "dependencies": {
    "esm": "^3.2.22"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "cross-env": "^5.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "nyc": "^14.0.0",
    "standard": "^12.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ],
    "color": true,
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": true,
    "verbose": true,
    "compileEnhancements": true,
    "require": [
      "esm"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "includes": [
      "\\.js$"
    ],
    "excludes": [
      "\\.config\\.js$"
    ],
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "lint": {
            "enable": true
          },
          "coverage": {
            "enable": true
          },
          "accessor": {
            "access": [
              "public",
              "protected",
              "private"
            ],
            "autoPrivate": true
          },
          "undocumentIdentifier": {
            "enable": true
          },
          "unexportedIdentifier": {
            "enable": false
          },
          "typeInference": {
            "enable": true
          },
          "brand": {
            "logo": "",
            "title": "linkedfunclist",
            "description": "A linked list whose elements store functions.",
            "repository": "https://github.com/foo/bar",
            "site": "http://my-library.org",
            "author": "https://github.com/anwarhahjjeffersongeorge",
            "image": "http://my-library.org/logo.png"
          },
          "manual": {
            "index": "./README.md",
            "globalIndex": true,
            "asset": "./manual/asset",
            "files": [
              "./manual/specification.md",
              "./manual/usage.md"
            ]
          },
          "test": {
            "source": "./test/",
            "interfaces": [
              "describe",
              "it",
              "context",
              "suite",
              "test"
            ],
            "includes": [
              "(spec|Spec|test|Test)\\.js$"
            ],
            "excludes": [
              "\\.config\\.js$"
            ]
          }
        }
      }
    ]
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "node_modules/**",
      "test/**"
    ],
    "include": [
      "src/**/*.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": "html"
  }
}
