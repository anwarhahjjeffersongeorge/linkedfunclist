{
  "name": "linkedfunclist",
  "version": "1.0.0",
  "description": "A linked list whose elements store functions.",
  "main": "./src/index.js",
  "module": "./src/main.js",
  "scripts": {
    "clean": "rm -rf .nyc_output/* coverage/* docs/*",
    "predoc": "rm -rf docs/*",
    "doc": "esdoc",
    "pretestr": "clear && standard --fix \"index.js\" \"main.js\"",
    "testr": "cross-env NODE_ENV=test nyc --require @babel/register --reporter=text --reporter=html ava",
    "pretest": "clear",
    "test": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "linked",
    "list",
    "promise",
    "function"
  ],
  "author": "Anwar Hahj Jefferson-George <anwarhahjjeffersongeorge@gmail.com>",
  "license": "CC-BY-SA-4.0",
  "dependencies": {
    "esm": "^3.2.22"
  },
  "devDependencies": {
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "babel-plugin-istanbul": "^5.1.2",
    "cross-env": "^5.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "nyc": "^14.0.0",
    "standard": "^12.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ],
    "color": true,
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": true,
    "verbose": true,
    "compileEnhancements": true,
    "require": [
      "esm"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "includes": ["\\.js$"],
    "excludes": ["\\.config\\.js$"],
    "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "accessor": {"access": ["public", "protected", "private"], "autoPrivate": true},
        "undocumentIdentifier": {"enable": true},
        "unexportedIdentifier": {"enable": false},
        "typeInference": {"enable": true},
        "brand": {
          "logo": "",
          "title": "linkedfunclist",
          "description": "A linked list whose elements store functions.",
          "repository": "https://github.com/foo/bar",
          "site": "http://my-library.org",
          "author": "https://github.com/anwarhahjjeffersongeorge",
          "image": "http://my-library.org/logo.png"
        },
        "manual": {
          "index": "./README.md",
          "globalIndex": true,
          "asset": "./manual/asset",
          "files": [
            "./manual/specification.md",
            "./manual/usage.md"
          ]
        },
        "test": {
          "source": "./test/",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"],
          "excludes": ["\\.config\\.js$"]
        }
      }
    }]
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "node_modules/**",
      "test/**"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": "html"
  }
}
